// .env.development - CONFIGURATION PERFORMANCE OPTIMISÉE

# ✅ API CONFIGURATION
VITE_API_URL=http://localhost:8000/api
VITE_ENABLE_MOCKS=true

# ✅ OPTIMISATIONS PERFORMANCE CRITIQUES - DÉSACTIVÉES
VITE_REDUCE_LOGS=true
VITE_VERBOSE_LOGS=false
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_ENABLE_CONSOLE_LOGS=false
VITE_DEBUG_MODE=false

# ✅ OPTIMISATIONS REDUX/REACT DEVTOOLS
VITE_ENABLE_REDUX_DEVTOOLS=false
VITE_DISABLE_REACT_DEVTOOLS=false

# ✅ OPTIMISATIONS RÉSEAU ET CACHE
VITE_MOCK_DELAY=25
VITE_ENABLE_CACHE=true
VITE_VALIDATION_CACHE_SIZE=30
VITE_TOKEN_CACHE_SIZE=10

# ✅ OPTIMISATIONS CANVAS KONVA
VITE_CANVAS_MAX_WIDTH=800
VITE_CANVAS_MAX_HEIGHT=600
VITE_DISABLE_GRID_ANIMATION=true
VITE_CANVAS_PERFECT_DRAW=false
VITE_MAX_TABLES_RENDER=50
VITE_MAX_OBSTACLES_RENDER=20

# ✅ OPTIMISATIONS HMR ET BUILD
VITE_HMR_OVERLAY=false
VITE_HMR_TIMEOUT=3000
VITE_MINIFY=true
VITE_SOURCEMAP=false

# ✅ THROTTLING DES ÉVÉNEMENTS (en ms)
VITE_CLICK_THROTTLE=100
VITE_DRAG_THROTTLE=150
VITE_RESIZE_THROTTLE=200
VITE_KEYBOARD_THROTTLE=200

# ✅ OPTIMISATIONS MÉMOIRE
VITE_COMPONENT_CACHE_SIZE=50
VITE_PERMISSION_CACHE_SIZE=100
VITE_RENDER_BATCH_SIZE=20

# ✅ DÉSACTIVER TOUTES LES FONCTIONNALITÉS DE DEBUG
VITE_ENABLE_DEBUG_PANEL=false
VITE_ENABLE_PERFORMANCE_PANEL=false
VITE_ENABLE_MEMORY_PANEL=false
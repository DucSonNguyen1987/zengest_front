# ✅ CONFIGURATION API OPTIMISÉE
VITE_API_URL=http://localhost:8000/api
VITE_ENABLE_MOCKS=true

# ✅ OPTIMISATIONS PERFORMANCE - ACTIVÉES
VITE_REDUCE_LOGS=true
VITE_VERBOSE_LOGS=false
VITE_ENABLE_PERFORMANCE_MONITORING=false

# ✅ OPTIMISATIONS DEVELOPMENT
VITE_ENABLE_REDUX_DEVTOOLS=false
VITE_DISABLE_REACT_DEVTOOLS=false

# ✅ OPTIMISATIONS RÉSEAU
VITE_MOCK_DELAY=50
VITE_ENABLE_CACHE=true

# ✅ OPTIMISATIONS CANVAS
VITE_CANVAS_MAX_WIDTH=800
VITE_CANVAS_MAX_HEIGHT=600
VITE_DISABLE_GRID_ANIMATION=true

# ✅ OPTIMISATIONS MÉMOIRE
VITE_VALIDATION_CACHE_SIZE=50
VITE_TOKEN_CACHE_SIZE=20

# ✅ OPTIMISATIONS HMR
VITE_HMR_OVERLAY=false
VITE_HMR_TIMEOUT=5000

# ✅ OPTIMISATIONS BUILD
VITE_MINIFY=true
VITE_SOURCEMAP=false

# ✅ MODE DEBUG (désactivé pour les performances)
VITE_DEBUG_MODE=false
VITE_ENABLE_CONSOLE_LOGS=false